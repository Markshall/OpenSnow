{
  "manifest_version": 2,
  "name": "OpenSnow",
  "description": "An open source global snowstorm script.",
  "version": "1.0",
  "author" : "griimnak",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js", "snowstorm.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],

  "permissions": [
    "<all_urls>"
  ]
}
